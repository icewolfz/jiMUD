// Place your settings in this file to overwrite default and user settings.
{
    "files.autoSave": "onFocusChange",
    "editor.rulers": [
        80
    ],
    "taskButtons.buttons": [
        {
            "task": "compile",
            "buttonLabel": "Compile"
        }
    ],
    "search.exclude": {
        "**/.git": true,
        "**/node_modules": true,
        "**/bower_components": true,
        "**/tmp": true,
        "**/_bak":true,
        "**/_raw":true,
        "**/_test":true,
        "**/lib":true,
        "**/build/css":true,
        "**/build/js":true,
        "**/src/js":true
    }
}